[teVMIDI.NTamd64]
Include=ks.inf,wdmaudio.inf
Needs=KS.Registration, WDMAUDIO.Registration
CopyFiles=teVMIDIDrv64.CopyList,teVMIDIInt64.CopyList,teVMIDIIntWOW.CopyList
AddReg=teVMIDI.AddReg64

[teVMIDI.NTx86.Services]
include=wdmaudio.inf
needs=wdmaudio.registration.services
AddService=sbvMIDI,2,teVMIDI_Service_Inst

[teVMIDI.NTamd64.Services]
include=wdmaudio.inf
needs=wdmaudio.registration.services
AddService=sbvMIDI,2,teVMIDI_Service_Inst64

[teVMIDI_Service_Inst]
DisplayName=%teVirtualMIDI.SvcDesc32%
ServiceType=1
StartType=3
ErrorControl=1
ServiceBinary=%12%\sbvmidi.sys

[teVMIDI_Service_Inst64]
DisplayName=%teVirtualMIDI.SvcDesc64%
ServiceType=1
StartType=3
ErrorControl=1
ServiceBinary=%12%\sbvmidi.sys

